.registers {
    A, B, C, D <- 3x3 builtin (30, 18, 10, 9)
}

    input "Which Fibonacci number to calculate: " D
    solved-goto D do_if_1
    goto after_if_1
do_if_1:
    halt "The number is: 0"
after_if_1:
    add B 1
loop_1:
    add D 8
    solved-goto D do_if_2
    goto after_if_2
do_if_2:
    halt "The number is: " B
after_if_2:
continue_1:
    solved-goto B break_1
    add B 17
    add A 1
    add C 1
    goto continue_1
break_1:
    add D 8
    solved-goto D do_if_3
    goto after_if_3
do_if_3:
    halt "The number is: " A
after_if_3:
continue_2:
    solved-goto A break_2
    add A 29
    add C 1
    add B 1
    goto continue_2
break_2:
    add D 8
    solved-goto D do_if_4
    goto after_if_4
do_if_4:
    halt "The number is: " C
after_if_4:
continue_3:
    solved-goto C break_3
    add C 9
    add B 1
    add A 1
    goto continue_3
break_3:
    goto loop_1
